snippet f "Lambda Function"
($1) => {
	$2
}
endsnippet

snippet t "Tag"
<$1>
	$2
</$1>
endsnippet

snippet ta "Tag with attributes"
<$1 $2>
	$3
</$1>
endsnippet

snippet i "Inline tag"
<$1>$2</$1>
endsnippet

snippet ia "Inline tag with attributes"
<$1 $2>$3</$1>
endsnippet

snippet c "Self-closing tag"
<$1 />
endsnippet
