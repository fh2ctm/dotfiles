snippet env "New Environment"
\begin{${1:align}}
  $2
\end{$1}
endsnippet

snippet enum "Enumerate"
\begin{enumerate}
  \item $1
\end{enumerate}
endsnippet

snippet frame "Frame"
\begin{frame}
  \frametitle{$1}
  \framesubtitle{$2}

  $3
\end{frame}
endsnippet

snippet item "Itemize"
\begin{itemize}
  \item $1
\end{itemize}
endsnippet
